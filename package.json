    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^2.0.0",
