{
  "name": "i-can-help-indeed.jp",
  "author": "Jakub Niewczas",
  "private": true,
  "version": "1.0.0",
  "description": "A mini game with visual novel flavour - how about starting a good relationship with a demo...",
  "engines": {
    "node": "14.8.0"
  },
  "scripts": {
    "dev": "yarn run dev:frontend",
    "dev:frontend": "webpack-dev-server --config ./src/webpack-frontend.config.js --port 3000",
